{
    "name": "jviba/yii2-cms-template",
    "description": "Yii 2 CMS Template",
    "keywords": ["cms", "yii2", "advanced", "project template"],
    "homepage": "https://github.com/djvibegga/yii2cmsjviba",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/djvibegga/yii2cmsjviba/issues",
        "wiki": "https://github.com/djvibegga/yii2cmsjviba/wiki",
        "source": "https://github.com/djvibegga/yii2cmsjviba"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "budyaga/yii2-cropper": "*",
        "dmstr/yii2-migrate-command": "*",
        "moonlandsoft/yii2-tinymce": "*",
        "paulzi/yii2-nested-sets": "^1.0",
        "kartik-v/yii2-widget-select2": "@dev",
        "inpassor/yii2-daemon": "^0.3.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
